{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "title": "Root Schema",
  "type": "object",
  "default": {},
  "required": [
    "id",
    "name",
    "username",
    "email",
    "address",
    "phone",
    "website",
    "company"
  ],
  "properties": {
    "id": {
      "title": "The id Schema",
      "type": "integer",
      "default": 0
    },
    "name": {
      "title": "The name Schema",
      "type": "string",
      "default": ""
    },
    "username": {
      "title": "The username Schema",
      "type": "string",
      "default": ""
    },
    "email": {
      "title": "The email Schema",
      "type": "string",
      "default": ""
    },
    "address": {
      "title": "The address Schema",
      "type": "object",
      "default": {},
      "required": [
        "street",
        "suite",
        "city",
        "zipcode",
        "geo"
      ],
      "properties": {
        "street": {
          "title": "The street Schema",
          "type": "string",
          "default": ""
        },
        "suite": {
          "title": "The suite Schema",
          "type": "string",
          "default": ""
        },
        "city": {
          "title": "The city Schema",
          "type": "string",
          "default": ""
        },
        "zipcode": {
          "title": "The zipcode Schema",
          "type": "string",
          "default": ""
        },
        "geo": {
          "title": "The geo Schema",
          "type": "object",
          "default": {},
          "required": [
            "lat",
            "lng"
          ],
          "properties": {
            "lat": {
              "title": "The lat Schema",
              "type": "string",
              "default": ""
            },
            "lng": {
              "title": "The lng Schema",
              "type": "string",
              "default": ""
            }
          }
        }
      }
    },
    "phone": {
      "title": "The phone Schema",
      "type": "string",
      "default": ""
    },
    "website": {
      "title": "The website Schema",
      "type": "string",
      "default": ""
    },
    "company": {
      "title": "The company Schema",
      "type": "object",
      "default": {},
      "required": [
        "name",
        "catchPhrase",
        "bs"
      ],
      "properties": {
        "name": {
          "title": "The name Schema",
          "type": "string",
          "default": ""
        },
        "catchPhrase": {
          "title": "The catchPhrase Schema",
          "type": "string",
          "default": ""
        },
        "bs": {
          "title": "The bs Schema",
          "type": "string",
          "default": ""
        }
      }
    }
  }
}